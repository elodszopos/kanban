{
  "presets": ["react", "es2015"],
  "env": {
    "start": {
        "plugins": [
            ["react-transform", {
                 "transforms": [
                     {
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                     }
                 ]
             }]
        ]
    }
  }
}